<h5>Calcular ganancia entre</h5>
<%= form_tag reporte_compras_path, method: :get do %>
<div class="form-group">
    <%= date_field_tag :inicio, params[:inicio], class: "form-control", placeholder: "Inicio" %>
</div>
<div class="form-group">
    <%= date_field_tag :fin, params[:fin], class: "form-control", placeholder: "Fin" %>
</div>
<%= submit_tag "Calcular", name: nil, class: "btn btn-primary btn-md" %>
<% end %>
